{
  "name": "altro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "prepare": "husky install",
    "test": "jest --watch",
    "test:ci": "jest --ci --all",
    "test-commit": "jest --bail --findRelatedTests --passWithNoTests",
    "cache-docs": "node scripts/cache.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run cache-docs && git add cache/data.js"
    }
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "lottie-react": "^2.4.0",
    "next": "13.4.4",
    "normalize.css": "^8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.7",
    "react-schemaorg": "^2.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.1.3",
    "react-youtube": "^10.1.0",
    "rehype-katex": "^6.0.3",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sass": "^1.62.1",
    "schema-dts": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "autoprefixer": "^10.4.14",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.1",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.2",
    "typewriter-effect": "^2.19.0",
    "yaml-loader": "^0.8.0"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npm run cache-docs",
      "npm run format",
      "npm run lint"
    ]
  }
}
